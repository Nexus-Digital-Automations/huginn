          "+      queueLength: this.requestQueue.length,",
          "+      activeRequests: this.activeRequests,",
          "+      requestsInWindow: this.requestCount,",
          "+      windowTimeLeft: Math.max(0, this.windowSize - (Date.now() - this.windowStart))",
          "+    };",
          "+  }",
          " }",
